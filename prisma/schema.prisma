generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Emergency {
  id              String   @id
  emergencyTypeId String
  domain          String
  title           String

  // ğŸ“ LOCATION
  lat             Float
  lng             Float
  locationAccuracy Int
  locationSource   String

  // ğŸ“ CONTACT
  phoneNumber     String

  // ğŸ”„ STATE
  status          String

  // ğŸ‘¤ ASSIGNMENT
  assignedRole    String?
  responderId     String?

  // ğŸ•’ AUDIT
  createdAt       DateTime
  updatedAt       DateTime?
}


